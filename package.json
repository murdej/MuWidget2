{
  "name": "mu-widget",
  "version": "1.0.10",
  "description": "Simple javascript widget creation",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "demos": "npx parcel watch -d ./demo/dist/ --no-hmr --public-url . demo/index.html",
    "build-demos": "npx parcel build --no-content-hash -d ./demo/dist/ --public-url . demo/index.html",
    "build": "tsc ; cd src ; cat MuBinder.ts MuRouter.ts MuUIDs.ts MuWidget.ts StrParser.ts MuLabelFor.ts | node ../trim-eximport.js > ../dist/MuWidget-bundle.ts ; tsc --target ES2015 ../dist/MuWidget-bundle.ts ; rm ../dist/MuWidget-bundle.ts "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murdej/MuWidget2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/murdej/MuWidget2/issues"
  },
  "homepage": "https://github.com/murdej/MuWidget2#readme",
  "devDependencies": {
    "sass": "^1.47.0",
    "typescript": "^4.5.4"
  }
}
